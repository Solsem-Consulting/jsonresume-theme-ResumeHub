{{#if education}}
<section id="education">
	<h2>Education</h2>
	{{#each education}}
	<div class="education">
		<h3>{{studyType}} in {{area}} at <a href="{{url}}" target="_blank">{{institution}}</a></h3>
		<p><strong>Duration:</strong> {{startDate}} - {{endDate}}</p>
		<p>Score: {{score}}</p>
	</div>
	{{/each}}
</section>
{{/if}}