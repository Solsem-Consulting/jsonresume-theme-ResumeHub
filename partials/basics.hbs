{{#resume.basics}}
<h1>{{name}}</h1>
<h2>{{label}}</h2>
<section id="basics">
    <div class="contact">
        {{#if url}}
        <div class="website">
            <strong>Website | Nettside</strong>
            {{url}}
        </div>
        {{/if}}
        {{#if email}}
        <div class="email">
            <strong>Email | E-post</strong>
            {{email}}
        </div>
        {{/if}}
        {{#if phone}}
        <div class="phone">
            <strong>Phone | Telefon</strong>
            {{phone}}
        </div>
        {{/if}}
    </div>
    {{#if summary}}
    <div class="summary">
        <p>{{summary}}</p>
    </div>
    {{/if}}
    {{#location}}
    <h3>Location | Sted</h3>
    <section id="location">
        {{#if address}}
        <div class="address">
            <strong>Address | Adresse</strong>
            {{address}}
        </div>
        {{/if}}
        {{#if postalCode}}
        <div class="postalCode">
            <strong>Postal code | Postnr</strong>
            {{postalCode}}
        </div>
        {{/if}}
        {{#if city}}
        <div class="city">
            <strong>City | Poststed</strong>
            {{city}}
        </div>
        {{/if}}
        {{#if countryCode}}
        <div class="countryCode">
            <strong>Country code | Landkode</strong>
            {{countryCode}}
        </div>
        {{/if}}
        {{#if region}}
        <div class="region">
            <strong>Region | Fylke</strong>
            {{region}}
        </div>
        {{/if}}
    </section>
    {{/location}}
    
    {{#if profiles.length}}
    <h3>Profiles | Profiler</h3>
    <section id="profiles">
        {{#each profiles}}
        <div class="item">
            {{#if network}}
            <strong class="network">
                {{network}}
            </strong>
            {{/if}}
            {{#if username}}
            <div class="username">
                {{username}}
            </div>
            {{/if}}
            {{#if url}}
            <div class="url">
                <a href="{{url}}">Link</a>
            </div>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}
</section>
{{/resume.basics}}