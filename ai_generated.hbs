<h1>{{basics.name}}</h1>
<h2>{{basics.label}}</h2>
<p>{{basics.summary}}</p>

<h3>Contact</h3>
<ul>
  <li>Email: {{basics.email}}</li>
  <li>Phone: {{basics.phone}}</li>
  <li>Website: <a href="{{basics.url}}">{{basics.url}}</a></li>
</ul>

<h3>Location</h3>
<p>{{basics.location.address}}, {{basics.location.city}}, {{basics.location.region}}, {{basics.location.countryCode}}, {{basics.location.postalCode}}</p>

<h3>Profiles</h3>
<ul>
  {{#each basics.profiles}}
    <li>{{network}}: <a href="{{url}}">{{username}}</a></li>
  {{/each}}
</ul>

<h3>Work Experience</h3>
<ul>
  {{#each work}}
    <li>
      <h4>{{position}} at {{name}}</h4>
      <p><strong>Location:</strong> {{location}}</p>
      <p><strong>Start Date:</strong> {{startDate}}</p>
      <p><strong>End Date:</strong> {{endDate}}</p>
      <p>{{summary}}</p>
      <ul>
        {{#each highlights}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
    </li>
  {{/each}}
</ul>

<h3>Volunteer Experience</h3>
<ul>
  {{#each volunteer}}
    <li>
      <h4>{{position}} at {{organization}}</h4>
      <p><strong>Start Date:</strong> {{startDate}}</p>
      <p><strong>End Date:</strong> {{endDate}}</p>
      <p>{{summary}}</p>
      <ul>
        {{#each highlights}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
    </li>
  {{/each}}
</ul>

<h3>Education</h3>
<ul>
  {{#each education}}
    <li>
      <h4>{{studyType}} in {{area}} - {{institution}}</h4>
      <p>Score: {{score}}</p>
      <ul>
        {{#each courses}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
    </li>
  {{/each}}
</ul>

<h3>Awards</h3>
<ul>
  {{#each awards}}
    <li>
      <h4>{{title}}</h4>
      <p><strong>Awarded by:</strong> {{awarder}}</p>
      <p><strong>Date:</strong> {{date}}</p>
      <p>{{summary}}</p>
    </li>
  {{/each}}
</ul>

<h3>Publications</h3>
<ul>
  {{#each publications}}
    <li>
      <h4><a href="{{url}}">{{name}}</a></h4>
      <p><strong>Published by:</strong> {{publisher}}</p>
      <p><strong>Release Date:</strong> {{releaseDate}}</p>
      <p>{{summary}}</p>
    </li>
  {{/each}}
</ul>

<h3>Skills</h3>
<ul>
  {{#each skills}}
    <li>
      <h4>{{name}} - {{level}}</h4>
      <ul>
        {{#each keywords}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
    </li>
  {{/each}}
</ul>

<h3>Languages</h3>
<ul>
  {{#each languages}}
    <li>{{language}} - {{fluency}}</li>
  {{/each}}
</ul>

<h3>Interests</h3>
<ul>
  {{#each interests}}
    <li>
      <h4>{{name}}</h4>
      <ul>
        {{#each keywords}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
    </li>
  {{/each}}
</ul>

<h3>References</h3>
<ul>
  {{#each references}}
    <li>
      <h4>{{name}}</h4>
      <p>{{reference}}</p>
    </li>
  {{/each}}
</ul>

<h3>Projects</h3>
<ul>
  {{#each projects}}
    <li>
      <h4><a href="{{url}}">{{name}}</a></h4>
      <p><strong>Entity:</strong> {{entity}}</p>
      <p><strong>Type:</strong> {{type}}</p>
      <p>{{description}}</p>
      <ul>
        {{#each highlights}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
      <p><strong>Keywords:</strong> {{#each keywords}} {{this}}, {{/each}}</p>
      <p><strong>Roles:</strong> {{#each roles}} {{this}}, {{/each}}</p>
      <p><strong>Start Date:</strong> {{startDate}}</p>
      <p><strong>End Date:</strong> {{endDate}}</p>
    </li>
  {{/each}}
</ul>

<h3>Metadata</h3>
<p><strong>Canonical:</strong> <a href="{{meta.canonical}}">{{meta.canonical}}</a></p>
<p><strong>Version:</strong> {{meta.version}}</p>
<p><strong>Last Modified:</strong> {{meta.lastModified}}</p>